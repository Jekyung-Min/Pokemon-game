  },
  "homepage": "https://github.com/codestates/im10-2019-01-subclass-dance-party#readme",
  "devDependencies": {
    "@mochajs/json-file-reporter": "^1.3.0",
    "chai": "^4.2.0",
    "eslint": "^5.11.1",
    "husky": "^0.14.3",
    "jsdom": "^12.0.0",
    "lint-staged": "^7.2.2",
    "mocha": "^8.2.1",
    "mocha-jsdom": "^2.0.0",
    "sinon": "^6.3.5"
