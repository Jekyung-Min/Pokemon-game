  "description": "",
  "main": ".eslintrc.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "mocha spec/test.js",
    "precommit": "lint-staged",
    "lint": "eslint src",
    "report": "mocha spec/test.js --reporter @mochajs/json-file-reporter",
